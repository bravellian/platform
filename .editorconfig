# ==================================================
# == MINIMAL BASELINE .editorconfig               ==
# == Goal: Disable analysis/style warnings/errors ==
# == Re-enable rules gradually by changing 'none' ==
# == back to 'suggestion', 'warning', or 'error'  ==
# ==================================================
root = true

# All files - Keep basic formatting
[*]
charset = utf-8
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = crlf
tab_width = 4
indent_size = 4# Keep consistent indent size

# --- Disabled Style Rules ---
dotnet_style_qualification_for_field = true:none # WAS suggestion
dotnet_style_qualification_for_property = true:none # WAS suggestion
dotnet_style_qualification_for_method = true:none # WAS suggestion
dotnet_style_qualification_for_event = true:none # WAS suggestion
dotnet_style_require_accessibility_modifiers = for_non_interface_members:none # WAS suggestion
dotnet_style_coalesce_expression = true:none # WAS suggestion
dotnet_style_null_propagation = true:none # WAS suggestion
dotnet_style_prefer_is_null_check_over_reference_equality_method = true:none # WAS suggestion
dotnet_style_prefer_auto_properties = true:none # WAS silent
dotnet_style_object_initializer = true:none # WAS suggestion
dotnet_style_prefer_collection_expression = when_types_exactly_match:none # WAS suggestion
dotnet_style_collection_initializer = true:none # WAS suggestion
dotnet_style_prefer_simplified_boolean_expressions = true:none # WAS suggestion
dotnet_style_prefer_conditional_expression_over_assignment = true:none # WAS silent
dotnet_style_prefer_conditional_expression_over_return = true:none # WAS silent
dotnet_style_explicit_tuple_names = true:none # WAS suggestion
dotnet_style_prefer_inferred_tuple_names = true:none # WAS suggestion
dotnet_style_prefer_inferred_anonymous_type_member_names = false:none # WAS suggestion
dotnet_style_prefer_compound_assignment = true:none # WAS suggestion
dotnet_style_prefer_simplified_interpolation = true:none # WAS suggestion
dotnet_style_namespace_match_folder = true:none # WAS suggestion
dotnet_style_readonly_field = true:none # WAS suggestion
dotnet_style_predefined_type_for_locals_parameters_members = true:none # WAS suggestion
dotnet_style_predefined_type_for_member_access = true:none # WAS suggestion
dotnet_style_allow_multiple_blank_lines_experimental = false:none # WAS silent
dotnet_style_allow_statement_immediately_after_block_experimental = false:none # WAS silent
dotnet_code_quality_unused_parameters = all:none # WAS suggestion
dotnet_style_parentheses_in_arithmetic_binary_operators = always_for_clarity:none # WAS silent
dotnet_style_parentheses_in_other_binary_operators = always_for_clarity:none # WAS silent
dotnet_style_parentheses_in_relational_binary_operators = always_for_clarity:none # WAS silent
dotnet_style_parentheses_in_other_operators = never_if_unnecessary:none # WAS silent

# --- Disabled Diagnostic Rules ---
dotnet_diagnostic.CA1068.severity = none # WAS warning

# --- Keep Resharper properties (if used) ---
dotnet_style_operator_placement_when_wrapping = beginning_of_line

# JSON files - Keep formatting
[*.json]
indent_size = 2

# Markdown files - Keep formatting
[*.md]
indent_size = 2
trim_trailing_whitespace = false

# PowerShell scripts - Keep formatting
[*.ps1]
indent_size = 4

# Visual Studio XML project files - Keep formatting
[*.{csproj,vcxproj,vcxproj.filters,proj,projitems,shproj}]
indent_size = 2

# Visual Studio and .NET related XML config files - Keep formatting
[*.{props,targets,ruleset,config,nuspec,resx,vsixmanifest,vsct}]
indent_size = 2

# YAML files - Keep formatting
[*.{yml,yaml}]
indent_size = 2
indent_style = space

# SQL Files - Keep formatting
[*.sql]
keyword_standardization = true
spaces_per_tab = 4
expand_comma_lists = true
trailing_commas = true
space_after_expanded_comma = true
uppercase_keywords = true
break_join_on_sections = false
expand_boolean_expressions = true
expand_between_conditions = false
expand_case_statements = true
expand_in_lists = true
max_line_width = 999
indent_string = "   "
new_clause_line_breaks = true
new_statement_line_breaks = true

# CSHTML Code Behind - Keep specific suppression if needed
[*.cshtml.cs]
dotnet_diagnostic.sa1649.severity = none # Ignore the ASP.NET Core Razor Page convention naming scheme.

#==================================================================
# C# files
#==================================================================
[*.cs]
indent_size = 4
indent_style = space # Use soft tabs (spaces) for indentation.

[**/*.g.cs]
dotnet_diagnostic.CS1591.severity = none

[**/*.g.i.cs]
dotnet_diagnostic.CS1591.severity = none

